{{ 'product-details.css' | asset_url | stylesheet_tag }}
{{ 'product.css' | asset_url | stylesheet_tag }}
{{ 'glightbox.min.css' | asset_url | stylesheet_tag }}
{%- capture contentForQuerystring -%}{{ content_for_header }}{%- endcapture -%}
{%- assign pageUrl = contentForQuerystring
  | split: '"pageurl":"'
  | last
  | split: '"'
  | first
  | split: request.host
  | last
  | replace: '\/', '/'
  | replace: '%20', ' '
  | replace: '\u0026', '&'
-%}
{%- assign pageQuerystring = pageUrl | split: '?' | last -%}
{%- liquid
  assign st = section.settings
  assign th_st = settings
  assign show_wishlist = th_st.show_wishlist
  assign show_compare = th_st.show_compare
  assign product_label_style = th_st.product_label_style
  assign design = st.design
  if pageQuerystring contains 'design='
    assign design_pr = pageQuerystring | split: 'design=' | last | split: '&' | first
    if design_pr == 'grid-1' or design_pr == 'grid-2' or design_pr == 'slide-full' or design_pr == 'thumbnails-bottom' or design_pr == 'thumbnails-left' or design_pr == 'thumbnails-right' or design_pr == 'no-thumbnails'
      assign design = design_pr
    endif
  endif
  assign bought_together = st.bought_together
  if pageQuerystring contains 'bought_together='
    assign bought_together_pr = pageQuerystring | split: 'bought_together=' | last | split: '&' | first
    if bought_together_pr == 'enable'
      assign bought_together = true
    endif
  endif
  assign sticky = st.sticky
  if pageQuerystring contains 'sticky='
    assign sticky_pr = pageQuerystring | split: 'sticky=' | last | split: '&' | first
    if sticky_pr == 'enable'
      assign sticky = true
    endif
  endif
  assign current_variant = product.selected_or_first_available_variant
  assign product_qty = 0
  assign prod = product.variants
  assign color = false
  assign size = false
  for variant in prod
    assign product_qty = product_qty | plus: variant.inventory_quantity
  endfor
  case design
    when 'grid-1'
      assign col_images = 'col-lg-6 col-12'
      assign col_infors = 'col-lg-6 col-12'
    when 'grid-2'
      assign col_images = 'col-lg-6 col-12'
      assign col_infors = 'col-lg-6 col-12'
    when 'slide-full'
      assign col_images = 'col-12'
      assign col_infors = 'col-12 text-center flex flex-column align-items-center'
    else
      assign col_images = 'col-lg-6 col-12'
      assign col_infors = 'col-lg-6 col-12'
  endcase
  for option in product.options
    case option
      when 'Color'
        assign color = true
      when 'Size'
        assign size = true
    endcase
  endfor
-%}

<div
  class="bls__section bls__template-product"
  style="--bls-gutter-x: 3rem"
  data-section="{{ section.id }}"
  data-product-id="{{ product.id }}"
>
  <div class="{{ st.layout }}">
    <div class="row">
      <div class="{{ col_images }} bls__product-images bls__product-{{ design }}">
        {% render 'product-media', st: st, design: design %}
      </div>
      <div class="{{ col_infors }}">
        <div class="bls__product-details-infor {{ product_label_style }}">
          {%- for block in section.blocks %}
            {% assign bk_st = block.settings %}
            {% assign bk_id = block.id -%}
            {%- case block.type -%}
              {%- when 'product_label' -%}
                {%- liquid
                  assign target = product.selected_or_first_available_variant
                  assign compare_at_price = target.compare_at_price
                  assign price = target.price | default: 1999
                -%}
                {%- if compare_at_price > price and compare_at_price != blank -%}
                  <div
                    id="bls__product-label"
                    class="bls__product-label fs-12 pointer-events-none inline-block"
                  >
                    <div class="bls__sale-label">
                      -
                      {{-
                        compare_at_price
                        | minus: price
                        | times: 100.0
                        | divided_by: compare_at_price
                        | remove: '.0'
                        | round
                      -}}
                      %
                    </div>
                  </div>
                {%- endif -%}
              {%- when 'product_title' -%}
                <h1
                  class="bls__product-title mb-5"
                  style="--bls__product-title-size:{{bk_st.font_size}}px;--bls__product-title-color:{{bk_st.color}};"
                >
                  {{ product.title }}
                </h1>
              {%- when 'review_sold' -%}
                <div class="bls__product-review-sold flex flex-wrap align-items-center fs-14">
                  {%- if bk_st.show_review -%}
                    <div class="bls__pro-reviews mr-20">
                      <span class="shopify-product-reviews-badge" data-id="{{ product.id }}"></span>
                    </div>
                  {%- endif -%}
                  {%- if bk_st.show_sold_product -%}
                    <div class="bls__sold-product">
                      <div
                        class="bls__flash-sold"
                        data-flash-sold="{{ bk_st.sold }}"
                        data-flash-hours="{{ bk_st.hours }}"
                        style="display:none"
                      >
                        <div class="flex align-items-center">
                          <svg
                            class="mr-10"
                            width="12"
                            height="16"
                            viewBox="0 0 12 16"
                            fill="none"
                            xmlns="http://www.w3.org/2000/svg"
