                      <div class="bls__product-dynamic-checkout">
                        {%- if block.settings.show_buy_now -%}
                          {%- if th_st.enable_product_terms_conditions -%}
                            <div class="bls__terms-conditions flex mb-25">
                              <div class="checkbox-group relative">
                                <input
                                  class="input m-0 opacity-0 absolute inset-0 pointer inset-0"
                                  type="checkbox"
                                  name="conditions"
                                  id="product_conditions_form"
                                >
                                <span class="checkmark"></span>
                              </div>
                              <label class="label form-label pointer" for="product_conditions_form">
                                {{ settings.content_terms_conditions }}
                              </label>
                            </div>
                          {%- endif -%}
                          <div class="bls__payment-button{% if settings.enable_product_terms_conditions %} disabled{% endif %}">
                            {{ form | payment_button }}
                          </div>
                        {%- endif -%}
                      </div>
                    {%- endform -%}
                  </product-form>
                </div>
              {%- when 'countdown_timer' -%}
                {%- if product.metafields.bls.countdown_timer -%}
                  <div
                    class="bls__countdown-timer hidden mb-25 d-none"
                    data-days="{{ 'general.times.days' | t }}"
                    data-hrs="{{ 'general.times.hrs' | t }}"
                    data-mins="{{ 'general.times.mins' | t }}"
                    data-secs="{{ 'general.times.secs' | t }}"
                    data-time="{{ product.metafields.bls.countdown_timer }}"
                  >
                    {%- if bk_st.message -%}
                      <div class="des-countdown-p-p-c heading-weight mb-10">
                        <p class="message">{{ bk_st.message }}</p>
                      </div>
                    {%- endif -%}
                    <div class="bls__product-countdown"></div>
                  </div>
                {%- endif -%}
              {%- when 'product_meta' -%}
                <ul class="bls__product-meta pl-0 list-none my-25 pt-25 border-top capitalize">
                  {% if bk_st.show_sku %}
                    <li>
                      <span class="bls__product-meta-label">{{ 'products.product.sku' | t }}:</span>
                      <span class="bls__product-meta-value heading-color prod__sku bls__product-sku-value">
                        {{- current_variant.sku | default: 'N/A' -}}
                      </span>
                    </li>
                  {% endif %}
                  {%- if bk_st.show_available -%}
                    <li>
                      {%- liquid
                        assign lable_stock = 'products.product.instock' | t
                        if current_variant.inventory_management == 'shopify'
                          if current_variant.available and current_variant.inventory_quantity < 1
                            assign lable_stock = 'products.product.pre_order' | t
                          elsif current_variant.available
                            assign lable_stock = 'products.product.instock' | t
                          else
                            assign lable_stock = 'products.product.outstock' | t
                          endif
                        endif
                      -%}
                      <span class="bls__product-meta-label">{{ 'products.product.available' | t }}:</span>
                      <span class="bls__product-meta-value bls__availability-value heading-color">
                        {{- lable_stock -}}
                      </span>
                    </li>
                  {%- endif -%}
                  {% if bk_st.show_vendors and product.vendor != blank %}
                    <li>
                      <span class="bls__product-meta-label">{{ 'products.product.vendor' | t }}:</span>
                      <span class="bls__product-meta-value heading-color">{{ product.vendor | link_to_vendor }}</span>
                    </li>
                  {% endif %}
                  {% if bk_st.show_type and product.type != blank %}
                    <li>
                      <span class="bls__product-meta-label">{{ 'products.product.type' | t }}:</span>
                      <span class="bls__product-meta-value heading-color">{{ product.type | link_to_type }}</span>
                    </li>
                  {% endif %}
                  {% if bk_st.show_collection and product.collections.size > 0 %}
                    <li class="bls__product-collections">
                      <span class="bls__product-meta-label">{{ 'products.product.collections' | t }}:</span>
                      <span class="bls__product-meta-value heading-color">
                        {% for collection in product.collections %}
                          <a href="{{collection.url}}" class="hover:text-black">
                            {{ collection.title }}
                            {%- if forloop.last != true -%},{%- endif -%}
                          </a>
                        {% endfor %}
                      </span>
                    </li>
                  {% endif %}
                </ul>
              {%- when 'trust_badge' -%}
                {% if bk_st.image != blank %}
                  {% assign position = bk_st.position %}
                  {%- liquid
                    assign heading_position = ''
                    if block.settings.position == 'below'
                      assign heading_position = ' pt-20 pb-25'
                    elsif block.settings.position == 'above'
                      assign heading_position = ' pt-25 pb-20'
                    endif
                  -%}
                  <div class="bls__trust_badge mt-30 mb-25 rounded-5 relative border text-center {{ heading_position }}">
                    <div class="trust_badge-image m-auto" style="width: {{ block.settings.image_width }}px">
                      {% render 'responsive-image', image: bk_st.image %}
                    </div>
                    {% if bk_st.heading != blank %}
                      <h6
                        class="trust_badge-text mb-0 absolute bg-white px-20 limited-line text__position-{{ position | default: 'below' }}"
                        style="--line-clamp: 2"
                      >
                        {{ bk_st.heading }}
                      </h6>
                    {% endif %}
                  </div>
                {% endif %}
